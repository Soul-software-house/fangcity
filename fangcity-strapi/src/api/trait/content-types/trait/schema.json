{
  "kind": "collectionType",
  "collectionName": "traits",
  "info": {
    "singularName": "trait",
    "pluralName": "traits",
    "displayName": "Trait",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "rarity": {
      "type": "enumeration",
      "enum": [
        "Common",
        "Rare",
        "Legendary"
      ],
      "required": true,
      "default": "Common"
    },
    "priceAwoo": {
      "type": "float",
      "required": true
    },
    "priceEth": {
      "type": "float",
      "required": true
    },
    "layer": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "maxQuantity": {
      "type": "integer"
    },
    "stock": {
      "type": "integer",
      "required": true
    },
    "overrideOrder": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "noEar": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "series": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::serie.serie",
      "inversedBy": "traits"
    },
    "requiredTraits": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::trait.trait",
      "mappedBy": "leave_this_empty"
    },
    "leave_this_empty": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::trait.trait",
      "inversedBy": "requiredTraits"
    }
  }
}
